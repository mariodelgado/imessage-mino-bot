    {
      "id": "doc-4fce12db",
      "source_id": "iscc",
      "url": "https://www.iscc-system.org/updates/22-december-2025/#1-2-iscc-eu-recognition-for-netherlands-sde",
      "title": "ISCC EU Recognition for Netherlands SDE++",
      "summary": "Starting January 1, 2026, economic operators can demonstrate compliance with the Dutch SDE++ biomass scheme via ISCC EU certification. The ISCC Solid Biomass NL scheme is now fully embedded within ISCC EU for energy applications.",
      "doc_type": "guidance",
      "content_hash": "936952ecf4ccc838",
